<?xml version="1.0"?>
<package >
  <metadata>
    <id>Swashbuckle</id>
    <version>$version$</version>
    <title>Swashbuckle - Swagger for WebApi</title>
    <authors>Richard Morris</authors>
    <licenseUrl>http://opensource.org/licenses/BSD-3-Clause</licenseUrl>
    <projectUrl>https://github.com/domaindrivendev/Swashbuckle</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Seamlessly adds a Swagger to WebApi projects!</description>
    <releaseNotes>Includes Swagger 2.0 and several enhancements to the config API</releaseNotes>
    <summary>Combines ApiExplorer and Swagger/swagger-ui to provide a rich discovery, documentation and playground experience to your API consumers</summary>
    <copyright>Copyright 2013</copyright>
    <tags>Swagger SwaggerUi Documentation Discovery Help WebApi AspNet AspNetWebApi Docs WebHost IIS</tags>
    <dependencies>
      <group targetFramework="net40">
        <!-- Common -->
        <dependency id="WebActivatorEx" />
        <dependency id="Swashbuckle.Core" version="5.0.5" />
        <!-- Net 4.0 -->
        <dependency id="Microsoft.AspNet.WebApi.WebHost" version="4.0.30506.0" />
      </group>
      <group targetFramework="net45">
        <!-- Common -->
        <dependency id="WebActivatorEx" />
        <dependency id="Swashbuckle.Core" version="5.0.5" />
        <!-- Net 4.5+ -->
        <dependency id="Microsoft.AspNet.WebApi.WebHost" version="5.2.3" />
      </group>
      <group targetFramework="net451">
        <!-- Common -->
        <dependency id="WebActivatorEx" />
        <dependency id="Swashbuckle.Core" version="5.0.5" />
        <!-- Net 4.5+ -->
        <dependency id="Microsoft.AspNet.WebApi.WebHost" version="5.2.3" />
      </group>
      <group targetFramework="net452">
        <!-- Common -->
        <dependency id="WebActivatorEx" />
        <dependency id="Swashbuckle.Core" version="5.0.5" />
        <!-- Net 4.5+ -->
        <dependency id="Microsoft.AspNet.WebApi.WebHost" version="5.2.3" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="Nuget\Content\App_Start\SwaggerConfig.cs.pp" target="Content\App_Start" />
  </files>
</package>